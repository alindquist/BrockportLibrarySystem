        </AnchorPane>
        <Pane fx:id="root" layoutX="225.0" prefHeight="750.0" prefWidth="1076.0">
            <children>
                <Text layoutX="375.0" layoutY="260.0" strokeType="OUTSIDE" strokeWidth="0.0" fx:id="welcome" textAlignment="CENTER" wrappingWidth="406.0">
                    <font>
                        <Font size="42.0" />
                    </font>
                </Text>
                <Text layoutX="948.0" layoutY="720.0" strokeType="OUTSIDE" strokeWidth="0.0" text="by Squad Two" textAlignment="RIGHT" wrappingWidth="102.5" />
            </children>
        </Pane>
    </children>
</Pane>
